{
  "name": "reveldigital-client-library",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve test-app",
    "build": "ng build test-app",
    "link:lib": "cd dist/reveldigital/player-client && npm link",
    "link:app": "cd projects/test-app && npm link @reveldigital/player-client",
    "link:all": "npm run link:lib && npm run link:app",
    "link:rm": "npm rm -g @reveldigital/player-client",
    "test-app:ng-add": "cd projects/test-app && ng g @reveldigital/player-client:ng-add",
    "build:lib": "ng build @reveldigital/player-client",
    "build:schematics": "npm run build --prefix projects/reveldigital/player-client",
    "build:all": "npm run build:lib && npm run build:schematics",
    "lib:all": "npm run build:all",
    "publish:all": "npm run lib:all && npm publish dist/reveldigital/player-client",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:gadget": "ng build --project test-app && node ./projects/test-app/utils/yml2xml.js ./projects/test-app/src/assets/gadget.yaml dist",
    "deploy:gadget": "npm run build:gadget && ng deploy --no-build",
    "change-path": "cd projects/test-app && node utils/changeBasePath.js",
    "compodoc:build": "compodoc -d ./docs -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -d ./docs -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -d ./docs -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/forms": "^13.0.0",
    "@angular/localize": "^13.0.0",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "classlist.js": "1.1.20150312",
    "rxjs": "~7.4.0",
    "systemjs": "6.13.0",
    "tslib": "^2.4.1",
    "webfontloader": "1.6.28",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.0",
    "@angular-devkit/schematics": "^13.0.0",
    "@angular/cli": "^13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@compodoc/compodoc": "^1.1.21",
    "@reveldigital/gadget-types": "1.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.6",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "git-remote-origin-url": "^4.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "js-yaml": "^4.1.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.0",
    "node-html-parser": "6.1.11",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.2",
    "xmlbuilder2": "^3.1.1"
  }
}
