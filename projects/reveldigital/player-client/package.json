{
  "name": "@reveldigital/player-client",
  "version": "2.0.0",
  "description": "Helper library for interfacing Angular apps with the Revel Digital gadget framework.",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/assets/** schematics/*/utils/** schematics/*/templates/** schematics/collection.json ../../../dist/reveldigital/player-client/"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "keywords": [
    "schematics",
    "angular",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RevelDigital/reveldigital-client-library.git"
  },
  "author": {
    "name": "Mike Tinnes"
  },
  "license": "",
  "peerDependencies": {
    "@angular/common": ">= 16.0.0",
    "@angular/core": ">= 16.0.0",
    "@reveldigital/gadget-types": "1.0.0",
    "webfontloader": "1.6.28",
    "js-yaml": "^4.1.0"
  },
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "copyfiles": "file:../../../node_modules/copyfiles",
    "typescript": "file:../../../node_modules/typescript"
  }
}
